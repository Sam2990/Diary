        <item name="colorPrimaryVariant">#9f93b6</item>
        <item name="colorOnPrimary">#fff6ff</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#d0c3e8</item>
        <item name="colorSecondaryVariant">#9f93b6</item>
